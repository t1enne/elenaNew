{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": [
      "@astrojs/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@src/*": [
        "src/*"
      ]
    }
  }
}
